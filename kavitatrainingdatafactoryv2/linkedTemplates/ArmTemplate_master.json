{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kavitatrainingdatafactoryv2"
		},
		"KavitaBlobMetaDataSqlConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KavitaBlobMetaDataSqlConnection'"
		},
		"DLSproviderFunctionConnection_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'DLSproviderFunctionConnection'"
		},
		"OrganizedDLSCOnnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'OrganizedDLSCOnnection'"
		},
		"DLSTargetProviderFunctionCOnnection_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://dlstargetprovider20201125160234.azurewebsites.net"
		},
		"KavitaTrainingKeyVaultConnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kavitatrainingkeyvaultv2.vault.azure.net/"
		},
		"DLSproviderFunctionConnection_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://dlstargetprovider20201125160234.azurewebsites.net"
		},
		"OrganizedDLSCOnnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kavitatrainingdls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kavitatrainingdatafactoryv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DLSproviderFunctionConnection_functionKey": {
						"value": "[parameters('DLSproviderFunctionConnection_functionKey')]"
					},
					"OrganizedDLSCOnnection_accountKey": {
						"value": "[parameters('OrganizedDLSCOnnection_accountKey')]"
					},
					"KavitaBlobMetaDataSqlConnection_connectionString": {
						"value": "[parameters('KavitaBlobMetaDataSqlConnection_connectionString')]"
					},
					"KavitaTrainingKeyVaultConnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('KavitaTrainingKeyVaultConnection_properties_typeProperties_baseUrl')]"
					},
					"DLSproviderFunctionConnection_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSproviderFunctionConnection_properties_typeProperties_functionAppUrl')]"
					},
					"OrganizedDLSCOnnection_properties_typeProperties_url": {
						"value": "[parameters('OrganizedDLSCOnnection_properties_typeProperties_url')]"
					},
					"DLSTargetProviderFunctionCOnnection_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSTargetProviderFunctionCOnnection_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}